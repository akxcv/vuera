{
  "name": "vuera",
  "description": "Use React in Vue and Vue in React",
  "version": "1.0.0",
  "repository": "akxcv/vuera",
  "author": "Alexander Komarov <ak@akxcv.com>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": ["dist"],
  "keywords": ["vue", "react", "interop", "adapter", "migrate", "seamless-integration"],
  "peerDependencies": {
    "@types/react": ">=16",
    "@types/react-dom": ">=16",
    "react": ">= 16",
    "react-dom": ">= 16",
    "vue": ">= 2.2"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.12.0",
    "@types/react": "16",
    "@types/react-dom": "16",
    "babel-jest": "^21.2.0",
    "microbundle": "^0.14.2",
    "typescript": "^4.5.5",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "react": "16",
    "react-dom": "16",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.2",
    "vue": "2.5"
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "lint": "eslint .",
    "test": "cross-env BABEL_ENV=test jest",
    "test:coverage": "yarn run test -- --coverage",
    "test:watch": "yarn run test -- --watch",
    "precommit": "lint-staged && yarn run build && git add dist"
  },
  "lint-staged": {
    "*.json": ["prettier --write", "git add"],
    "src/**/*.js": ["prettier --write", "eslint --fix", "git add"],
    "tests/**/*.js": ["prettier --write", "eslint --fix", "git add"],
    "/*.js": ["prettier --write", "eslint --fix", "git add"]
  }
}
