{
  "parser": "babel-eslint",
  "extends": ["umbrellio", "umbrellio/react"],
  "rules": {
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-single"],
    "new-cap": "off",
    "no-param-reassign": "off",
    "no-new": "off",
    "react/prop-types": "off"
  },
  "plugins": ["vue"],
  "globals": {
    "Symbol": true
  }
}
